<main>
    {{#each teas}}
    <div class="card" style="width: 40rem;">
        <img src="{{img}}">
        <div class="card-body">
            <h5 class="card-title">{{typeOfTea}}</h5>
            <p class="card-text">{{mood}}</p>
            <p>{{caffiene_content}}</p>
            <a href="#" class="btn btn-primary">Select This Tea</a>
        </div>
    </div>
    {{/each}}
</main>