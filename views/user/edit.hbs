<main>

	<h2>Edit Tree</h2>

	

	<div id="edit-form" class="">

    <form action="/{{user._id}}?_method=PATCH" method="POST">
      <div>
        <label for="name">Name: </label>
        <input type="text" name="name" value="{{user.name}}">
      </div>
      <div>
        <label for="description">Email: </label>
        <input type="email" name="description" value="{{user.email}}">
      </div>
      <button type="submit">Create New Donut</button>
    </form>


	</div>
    <a href="/">Return to Home Page</a>
</main>